  <script>
    $(document).ready(function() {
      // When a link is clicked
      $('.nav-link').on('click', function() {
        // Store the clicked link href into the localStorage
        localStorage.setItem('activeNavLink', $(this).attr('href'));
      });
      // Get the currently active link href from localStorage
      var activeNavLink = localStorage.getItem('activeNavLink');
      // If there's an active link stored
      if (activeNavLink) {
        // Remove 'shadow' class from all nav-links
        $('.nav-link').removeClass('shadow');  
        // Add 'shadow' class to the active nav-link
        $('.nav-link[href="' + activeNavLink + '"]').addClass('shadow');
      }
    });
  </script>
  

    
